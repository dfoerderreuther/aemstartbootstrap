<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Imagelink"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <category
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Category"
                        name="./category"
                        type="textfield"
                        xtype="textfield"/>
                    <name
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Name"
                        name="./name"
                        type="textfield"
                        xtype="textfield"/>
                    <link
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Link"
                        name="./link"
                        type="pathfield"
                        xtype="pathfield"/>
                    <image
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Image"
                        name="./image"
                        type="smartimage"
                        xtype="smartimage"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
