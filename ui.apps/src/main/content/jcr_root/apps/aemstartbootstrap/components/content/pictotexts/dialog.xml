<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    title="Section"
    xtype="dialog">
    <items
        jcr:primaryType="cq:Widget"
        xtype="tabpanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="Tab 1">
                <items jcr:primaryType="cq:WidgetCollection">
                    <name
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Navigation name"
                        name="./name"
                        xtype="textfield"/>
                    <cssclass
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Block CSS-Class"
                        name="./cssclass"
                        type="select"
                        xtype="selection">
                        <options jcr:primaryType="cq:WidgetCollection">
                            <empty
                                jcr:primaryType="nt:unstructured"
                                text="empty"
                                value=""/>
                            <primary
                                jcr:primaryType="nt:unstructured"
                                text="primary"
                                value="bg-primary"/>
                            <dark
                                jcr:primaryType="nt:unstructured"
                                text="dark"
                                value="bg-dark"/>
                        </options>
                    </cssclass>
                    <heading
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Headline"
                        name="./heading"
                        type="textfield"
                        xtype="textfield"/>
                    <text
                        jcr:primaryType="cq:Widget"
                        fieldLabel="Text"
                        name="./text"
                        type="richtext"
                        xtype="richtext"/>
                </items>
            </tab1>
        </items>
    </items>
</jcr:root>
